<style> 
	
	 .moveModal{
		display: flex;
		justify-content: center;
		align-items: center;
	 } 
	  ul {
		list-style: none;
	  }
	  
	  a {
		color: inherit;
		text-decoration: none;
	  }
	  
	  .menu {
		display: inline-flex;
		padding: 0 20px;
		background: transparent;
		box-shadow: 0 12px 8px rgb(0 0 0 / 8%);
		overflow: hidden;
		width: 40%;
		justify-content:center;
	  }
	  
	  .menu li:not(:last-child) {
		margin-right: 70px;
	  }
	  
	  .menu a {
		position: relative;
		display: grid;
		padding: 40px 20px;
		font-size: 14px;
		font-weight: bold;
		text-transform: uppercase;
	  }
	  
	  .menu a > * {
		grid-column: 1;
		grid-row: 1;
		transition: all 0.3s ease-out;
	  }
	  
	  .menu a span {
		transform: translateY(-60px);
	  }
	  
	  .menu a i {
		font-size: 5rem;
	  }
	  
	  .menu a:hover span {
		color: black;
		transform: translateY(-20px);
	  }
	  
	  .menu a:hover i {
		color: var(--black);
		transform: translateY(10px);
	  }


	.noBorder{border:none;}

	.bodycard{
		width: 100%; 
		height: 100vh;
	}
</style>