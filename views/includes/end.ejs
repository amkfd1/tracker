    document.getElementById('alert-message').style.display = 'none';
  }, 3000);
</script>
<script> 
    // Initialize Bloodhound for remote search
    const bloodhound = new Bloodhound({
    datumTokenizer: Bloodhound.tokenizers.obj.whitespace('Customer_Name'),
    queryTokenizer: Bloodhound.tokenizers.whitespace,
    remote: {
        url: 'https://m-kel-tech-tracker.onrender.com/track/search?input=%QUERY',
        wildcard: '%QUERY',
    },
    });

    // Initialize the typeahead input field
    $('#search-input').typeahead(
    {
        hint: true,
        highlight: true,
        minLength: 1,
    },
    {
        name: 'customers',
        display: 'Customer_Name',
        source: bloodhound,
        templates: {
        suggestion: function (data) {
            return `<div class="bg-dark"><a class="" href="/client/${data._id}">${data.Customer_Name}</a></div>`;
        },
        },
    }
    );
</script>
